<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.amp.user.dao.UserDao">

    <sql id="baseColumn">
      user_name,password,creator,create_time
    </sql>

    <insert id="saveUserInfo" parameterType="com.amp.user.model.UserInfo" keyProperty="id" useGeneratedKeys="true" >
        INSERT  INTO t_user(
        <include refid="baseColumn"/>
        )
        values (#{userName,jdbcType=VARCHAR},#{password,jdbcType=VARCHAR},#{creator,jdbcType=BIGINT},now())
    </insert>
</mapper>